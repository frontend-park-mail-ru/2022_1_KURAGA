each item in items
    div.papa
        div.element(data-section=item.key)
            object.svg(type="image/svg+xml", data=item.data, tabindex="-1")
            input.menu-input(placeholder=item.placeholder, type=item.type, required, data-section=item.key autocomplete="off")
        div.error(data-section=item.error)
div.error(data-section="incorrect")
